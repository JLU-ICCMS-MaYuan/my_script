    "                   sites=(['2a'], ['12d']),\n",
    "                   factor=1)\n",
    "print(struct)\n",
    "struct.to_file(\"CaH6.vasp\")\n",
    "density = struct.get_density()\n",
    "# print(density)\n",
    "H_flag = struct.check_H_coordination(r=2.0)\n",
    "\n",
    "pmg_struct = struct.to_pymatgen()"
